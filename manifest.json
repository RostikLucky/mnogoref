{
  "manifest_version": 2,
  "name": "MNOGOREF",
  "version": "1",
  "icons": {
    "128": "ico.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "activeTab",
    "http://*/",
    "https://*/",
    "cookies",
    "proxy",
    "contextMenus"
  ],
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "browser_action": {
    "default_title": "MNOGOREF"
  }
}
